{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",    
        "use": "@vercel/node"     
      },
      {
        "src": "index.html",  
        "use": "@vercel/static"   
      }
    ],
    "routes": [
      {
        "src": "/(.*)",       
        "dest": "api/index.js"
      },
      {
        "src": "/(.*)",           
        "dest": "/index.html"
      }
    ]
  }
  